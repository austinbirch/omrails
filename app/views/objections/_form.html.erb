<%= simple_form_for(@objection, html: { class: "form-horizontal"}) do |f| %>
  <%= f.error_notification %>

    <%= f.input :title, as: :text, input_html: { rows: "2" }  %>
    <%= f.input :objection, as: :text, input_html: { rows: "7" } %>
    <%= f.input :rebuttal, as: :text, input_html: { rows: "7" }%>
    <%= f.input :tag %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
